<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">

<ejb-jar>

	<display-name>PackageManager</display-name>

	<enterprise-beans>

		<!-- Session Beans -->
		<session>
			<display-name>PackageManagerBean</display-name>

			<ejb-name>PackageManagerBean</ejb-name>

			<home>org.openthinclient.pkgmgr.PackageManager</home>
			<remote>org.openthinclient.pkgmgr.PackageManager</remote>
			<ejb-class>
				org.openthinclient.pkgmgr.ejb.PackageManagerBean
			</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>

		</session>

	</enterprise-beans>
</ejb-jar>
